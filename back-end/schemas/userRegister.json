{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20 
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20 
    },
    "phone": {
      "type": "string",
      "minLength": 10,
      "maxLength": 20
    },
    "regCode": {
      "type": "string",
      "minLength": 1,
      "maxLength": 15
    },
     "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 15
    },
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 30,
      "format": "email"
    }
  },
  "additionalProperties": false,
  "required": [
    "firstName",
    "lastName",
    "phone",
    "regCode",
    "username",
    "password",
    "email"
  ]
}
